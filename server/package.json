{
  "name": "cardmatch-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json --skipLibCheck || true",
    "postbuild": "npx prisma db seed || true",
    "start": "node dist/index.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:seed-match": "ts-node prisma/seed-match-questions.ts",
    "load-test": "ts-node scripts/load-test.ts",
    "load-test:100": "ts-node scripts/load-test.ts 100",
    "load-test:1000": "ts-node scripts/load-test.ts 1000",
    "load-test:10000": "ts-node scripts/load-test.ts 10000"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.667.0",
    "@prisma/client": "^5.10.0",
    "agora-token": "^2.0.5",
    "axios": "^1.13.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "express-rate-limit": "^7.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.5",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^11.0.0",
    "prisma": "^5.10.0",
    "socket.io-client": "^4.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0"
  }
}
